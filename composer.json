{
    "name": "thingston/crawler",
    "type": "library",
    "description": "Web crawler based on PHP Guzzle HTTP Client with concurrency support for faster operation.",
    "keywords": ["php", "crawler"],
    "homepage": "https://thingston.com",
    "license": "MIT",
    "authors": [
        {"name": "Pedro Ferreira", "email": "pedro@thingston.com"}
    ],
    "require": {
        "php": "^7.1",
        "psr/http-message": "^1.0",
        "psr/log": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "t1gor/robots-txt-parser": "^0.2.4",
        "monolog/monolog": "^1.23",
        "symfony/dom-crawler": "^4.1",
        "symfony/css-selector": "^4.1",
        "jwage/purl": "^0.0.10",
        "jakubkulhan/chrome-devtools-protocol": "^1.0",
        "zendframework/zend-feed": "^2.10",
        "doctrine/dbal": "^2.8",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "symfony/var-dumper": "^4.1",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Thingston\\": "src/Thingston/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ThingstonTest\\": "test/"
        }
    }
}
